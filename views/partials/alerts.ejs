<% if (alerts.error) { %>
    <% alerts.error.forEach(msg => { %>
        <div class="alert container center-align blue darken-3">
            <%= msg %>
        </div>
    <%}) %>
    <%} %>
    <% if (alerts.success) { %>
        <% alerts.success.forEach(msg => { %>
        <div class="alert container center-align blue darken-3">
            <%= msg %>
        </div>
    <%}) %>
    <%} %>

    

    