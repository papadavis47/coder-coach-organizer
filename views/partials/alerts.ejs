<% if (alerts.error) { %>
    <% alerts.error.forEach(msg => { %>
        <div class="alert red container valign-wrapper center-align">
            <%= msg %>
        </div>
    <%}) %>
    <%} %>
    <% if (alerts.success) { %>
        <% alerts.success.forEach(msg => { %>
        <div class="alert green container valign-wrapper center-align">
            <%= msg %>
        </div>
    <%}) %>
    <%} %>

    

    