<h2 class="center-align">Your Saved Topics</h2>

<% topics.forEach(function(topic) { %>
    <div class="container">
        <h4>
            <a href="/topics/<%= topic.id %>">
             <%= topic.name %> 
            </a>
        </h4>
    </div>
    
<% }) %>

<hr>

<p><a href="/profile">Back to Profile</a></p>