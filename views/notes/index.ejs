<h1>Your Saved Notes</h1>

<% notes.forEach(function(note) { %>
    <div>
        <p>
            <a href="/notes/<%= note.id %>/edit">
             <%= note.content %> 
            </a>
        </p>
    </div>
    <form method="POST" action="/notes/<%= note.id %>/?_method=DELETE">
        <input class="btn" type="submit" value="Delete">
    </form>
    
<% }) %>

<hr>

<a href="/profile">Back to Profile</a>